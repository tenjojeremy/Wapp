{
  "name": "tenjo-apps",
  "version": "1.0.0",
  "scripts": {
    "portfolio:start": "PROJECT=portfolio node scripts/index.js start",
    "portfolio:build": "PROJECT=portfolio node scripts/index.js build",
    "portfolio:deploy": "npm run portfolio:build && cd projects/portfolio && firebase deploy && cd ..",
    "storybook:start": "start-storybook -p 9002 -c components/.storybook -s components/.storybook/public --quiet",
    "storybook:build": "build-storybook -c components/.storybook -o components/.storybook/_build -s components/.storybook/public",
    "storybook:deploy": "npm run storybook:build && firebase deploy",
    "components:new": "node components/.storybook/generators/component/index.js",
    "components:compile": "cd components && rm -rf _build && cd .. && cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config ./components --out-dir ./components/_build/ --ignore 'components/**/specs','components/**/stories' && node ./utils/babel/addPackageJson",
    "deploy": "npm run test && npm run deploy:only",
    "deploy:only": "npm run components:compile && npm run storybook:deploy && npm publish --minor && git push",
    "prettify": "prettier \"**/*.js\" --write",
    "test": "npm run test:jest && npm run test:cypress:open && npm run test:cypress:run && npm run test:cypress:run-ci && npm run test:lint",
    "test:jest": "jest",
    "test:cypress:open": "cypress open",
    "test:cypress:run": "cypress run --browser chrome",
    "test:cypress:run-ci": "start-server-and-test start http://localhost:9002 cy:run",
    "test:lint": "eslint ./"
  },
